{
  "ci": {
    "assert": {
      "preset": "lighthouse:no-pwa",
      "assertions": {
        "unused-javascript": ["warn", { "maxLength": 1 }],
        "non-composited-animations": "off",
        "uses-optimized-images": ["warn", { "maxLength": 2 }]
      }
    },
    "collect": {
      "staticDistDir": "./",
      "numberOfRuns": "1"
    }
  }
}
